  // ### console
  "debug.console.fontSize": 15,

  // ### diff
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,

  // ### editor
  "editor.codeActionsOnSave": { "source.fixAll": true, "source.fixAll.sortJSON": false },
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "Roboto Mono for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.linkedEditing": true,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "boundary",
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  // "editor.wordWrap": "on",
  // "editor.wordWrapColumn": 140,

  // ### editor.defaultFormatter
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[scss]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[vue]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  // "[csharp]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  // "[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  // "[php]": { "editor.defaultFormatter": "kokororin.vscode-phpfmt" },

  // ### emmet
  "emmet.showSuggestionsAsSnippets": true,
  // "editor.snippetSuggestions": "top"

  // ### eslint
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.format.enable": false,
  "eslint.run": "onSave",
  // "eslint.probe": ["javascript", "typescript", "vue"],
  // "eslint.validate": ["javascript", "typescript", "vue"],

  // ### exporler
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,

  // ### extensions
  "extensions.ignoreRecommendations": false,

  // ### indent rainbow
  // prettier-ignore
  "indentRainbow.colors": ["rgba(255, 215, 0,0.07)", "rgba(135, 206, 250,0.07)", "rgba(218, 112, 214,0.07)"],
  "indentRainbow.updateDelay": 500,

  // ### files specific
  // prettier-ignore
  "files.associations": { ".env": "properties", ".aliases": "properties", ".env.docker-local": "properties" },
  "files.autoGuessEncoding": true,
  // prettier-ignore
  "files.exclude": { 
    "files.trimFinalNewlines": true, 
    "files.trimTrailingWhitespace": true, 
    "**/*.code-search": true, 
    "**/*.psd": true, 
    "**/.cache": true, 
    "**/.next": true, 
    "**/.vs": true, 
    "**/bin": true, 
    "**/bower_components": true, 
    "**/dist": true, 
    "**/node_modules": true, 
    "**/obj": true, 
    // "**/public": true, 
    "**/vendor": true 
  },
  // "files.autoSave": "onFocusChange",
  // "files.autoSaveDelay": 500,

  // ### git
  "git.autofetch": true,
  "git.autoRepositoryDetection": "openEditors",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,

  // ### git lens
  "gitlens.blame.ignoreWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "typescript.updateImportsOnFileMove.enabled": "always",

  // ### peacock
  "peacock.darkenLightenPercentage": 1,
  // prettier-ignore
  "peacock.elementAdjustments": { "statusBar": "lighten", "titleBar": "lighten" },
